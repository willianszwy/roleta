    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.54.2",
    "@rollup/plugin-replace": "^6.0.2",
    "@testing-library/dom": "^10.4.1",
